{
	"log": {
		"loglevel": "warning"
	},
	"inbounds": [
		{
			"listen": "127.0.0.1",
			"port": "1080",
			"protocol": "socks",
			"settings": {
				"auth": "noauth",
				"udp": true,
				"ip": "127.0.0.1"
			}
		},
		{
			"listen": "127.0.0.1",
			"port": "1081",
			"protocol": "http"
		}
	],
	"outbounds": [
		{
			"protocol": "vless",
			"settings": {
				"vnext": [
					{
						"address": "1.2.3.4",
						"port": 443,
						"users": [
							{
								"id": "",
								"encryption": "none",
								"level": 0
							}
						]
					}
				]
			},
			"streamSettings": {
				"network": "gun",
				"security": "tls",
				"tlsSettings": {
					"serverName": "your.domain.com",
					"allowInsecure": false,
					"alpn": [
						"h2"
					],
					"disableSessionResumption": true
				},
				"grpcSettings": {
					"serviceName": "GunService"
				}
			}
		}
	]
}