{
    "access": {
        "type": "Console",
        "level": "Warning"
    },
    "error": {
        "type": "Console",
        "level": "Warning"
    },
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 12345,
            "protocol": "shadowsocks",
