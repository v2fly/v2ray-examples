{
    "access": {
        "type": "Console",
        "level": "Warning"
    },
    "error": {
        "type": "Console",
        "level": "Warning"
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 1234,
            "protocol": "shadowsocks",
            "settings": {
                "method": "chacha20-ietf-poly1305",
                "password": "{{ password }}"
            },
            "streamSettings": {
                "transport": "tcp"
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "tag": "block"
        }
    ]
}
